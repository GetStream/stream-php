{
    "name": "get-stream/stream",
    "description": "A PHP client for Stream (https://getstream.io)",
    "keywords": ["stream", "newsfeed", "feedly"],
    "homepage": "https://getstream.io",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tommaso Barbugli",
            "email": "support@getstream.io"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.0",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "99designs/http-signatures": ">=1.1.0 <3.0.0",
        "ocramius/package-versions": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.3",
        "satooshi/php-coveralls": "^1.0",
        "ramsey/uuid": "^3.7"
    },
    "autoload": {
        "psr-0": {
            "GetStream\\Stream" : "lib/"
        },
        "psr-4": {
            "GetStream\\Stubs\\" : "tests/stubs/",
            "GetStream\\Unit\\" : "tests/unit/",
            "GetStream\\Integration\\" : "tests/integration/"
        },
        "files": [
            "lib/GetStream/Stream/helpers.php"
        ]
    }
}
